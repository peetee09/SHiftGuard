rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /timesheets/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
    
    match /exports/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
    
    match /backups/{allPaths=**} {
      allow read, write: if request.auth != null && 
        request.auth.uid != null;
    }
  }
}
